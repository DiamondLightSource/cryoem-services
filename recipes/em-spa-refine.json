{
  "1": {
    "output": {
      "node_creator": 2,
      "refine_wrapper": 3
    },
    "parameters": {
      "class3d_dir": "{class3d_dir}",
      "nr_iter_3d": "{nr_iter}",
      "original_pixel_size": "{pixel_size}",
      "refine_class_nr": "{class_number}",
      "refine_job_dir": "{refine_job_dir}",
      "relion_options": {}
    },
    "queue": "extract_class",
    "service": "ExtractClass"
  },
  "2": {
    "queue": "node_creator",
    "service": "NodeCreator"
  },
  "3": {
    "job_parameters": {
      "mask_diameter": "{mask_diameter}",
      "particle_diameter": "{particle_diameter}",
      "relion_options": {},
      "symmetry": "{symmetry}"
    },
    "output": {
      "node_creator": 2,
      "postprocess": 4
    },
    "parameters": {
      "cluster": {
        "commands": [
          "source /etc/profile.d/modules.sh",
          "module load EM/cryoem-services",
          "export ZOCALO_CONFIG={zocalo_config_file}",
          "zocalo.wrap --wrap Refine3D --recipewrapper \"$RECIPEWRAP\" >runinfo 2>&1"
        ],
        "cpus_per_task": 8,
        "environment": {
          "PATH": "/bin/:/usr/bin/:/usr/local/bin/",
          "USER": "{user}"
        },
        "gpus": 4,
        "gpus_per_node": 4,
        "job_name": "Refine3D-EM",
        "min_memory_per_gpu": 4096,
        "nodes": 1,
        "partition": "{partition}",
        "prefer": "{partition_preference}",
        "scheduler": "slurm",
        "standard_error": "{refine_job_dir}/run.err",
        "standard_output": "{refine_job_dir}/run.out",
        "tasks": 5,
        "time_limit": "24:00:00"
      },
      "recipewrapper": "{refine_job_dir}/.recipewrap",
      "workingdir": "{refine_job_dir}/"
    },
    "queue": "cluster.submission",
    "service": "RefineWrapper",
    "wrapper": {
      "task_information": "Refine3D"
    }
  },
  "4": {
    "output": {
      "ispyb_connector": 6,
      "murfey_feedback": 5,
      "node_creator": 2,
      "refine_wrapper": 7
    },
    "parameters": {
      "picker_id": "{picker_id}",
      "refined_class_uuid": "{refined_class_uuid}",
      "refined_grp_uuid": "{refined_grp_uuid}"
    },
    "queue": "postprocess",
    "service": "PostProcess"
  },
  "5": {
    "parameters": {
      "program_id": "{autoproc_program_id}",
      "session_id": "{session_id}"
    },
    "queue": "{feedback_queue}",
    "service": "Murfey"
  },
  "6": {
    "parameters": {
      "dcid": "{dcid}",
      "ispyb_command": "multipart_message",
      "program_id": "{autoproc_program_id}"
    },
    "queue": "ispyb_connector",
    "service": "EMISPyB"
  },
  "7": {
    "job_parameters": {
      "mask_diameter": "{mask_diameter}",
      "particle_diameter": "{particle_diameter}"
    },
    "output": {
      "node_creator": 2,
      "postprocess": 8
    },
    "parameters": {
      "cluster": {
        "commands": [
          "source /etc/profile.d/modules.sh",
          "module load EM/cryoem-services",
          "export ZOCALO_CONFIG={zocalo_config_file}",
          "zocalo.wrap --wrap Refine3D --recipewrapper \"$RECIPEWRAP\" >runinfo 2>&1"
        ],
        "cpus_per_task": 8,
        "environment": {
          "PATH": "/bin/:/usr/bin/:/usr/local/bin/",
          "USER": "{user}"
        },
        "gpus": 4,
        "gpus_per_node": 4,
        "job_name": "Symmetry3D-EM",
        "min_memory_per_gpu": 4096,
        "nodes": 1,
        "partition": "{partition}",
        "prefer": "{partition_preference}",
        "scheduler": "slurm",
        "standard_error": "{refine_job_dir}/run_sym.err",
        "standard_output": "{refine_job_dir}/run_sym.out",
        "tasks": 5,
        "time_limit": "24:00:00"
      },
      "recipewrapper": "{refine_job_dir}/.recipewrap",
      "workingdir": "{refine_job_dir}/"
    },
    "queue": "cluster.submission",
    "service": "RefineWrapper",
    "wrapper": {
      "task_information": "Refine3D"
    }
  },
  "8": {
    "output": {
      "ispyb_connector": 6,
      "murfey_feedback": 5,
      "node_creator": 2
    },
    "parameters": {
      "picker_id": "{picker_id}",
      "refined_class_uuid": "{refined_class_uuid}",
      "refined_grp_uuid": "{refined_grp_uuid}"
    },
    "queue": "postprocess",
    "service": "PostProcess"
  },
  "start": [[1, []]]
}
