{
  "1": {
    "output": {
      "node_creator": 2,
      "refine_wrapper": 3
    },
    "parameters": {
      "batch_size": "{batch_size}",
      "bfactor_directory": "{bfactor_directory}",
      "class_number": "{class_number}",
      "mask": "{mask}",
      "number_of_particles": "{number_of_particles}",
      "pixel_size": "{pixel_size}",
      "relion_options": {},
      "rescaled_class_reference": "{class_reference}"
    },
    "queue": "bfactor",
    "service": "BFactor"
  },
  "2": {
    "queue": "node_creator",
    "service": "NodeCreator"
  },
  "3": {
    "job_parameters": {
      "mask_diameter": "{mask_diameter}",
      "particle_diameter": "{particle_diameter}",
      "relion_options": {}
    },
    "output": {
      "node_creator": 2,
      "postprocess": 4
    },
    "parameters": {
      "cluster": {
        "commands": [
          "source /etc/profile.d/modules.sh",
          "module load EM/cryoem-services",
          "cryoemservices.wrap -w Refine3D -r \"$RECIPEWRAP\" -c {config_file} >runinfo 2>&1"
        ],
        "cpus_per_task": 8,
        "environment": {
          "PATH": "/bin/:/usr/bin/:/usr/local/bin/",
          "USER": "{user}"
        },
        "gpus": 4,
        "job_name": "BFactor-EM",
        "memory_per_node": 16384,
        "nodes": 1,
        "tasks": 5,
        "time_limit": "12:00:00"
      },
      "wrapper": "{bfactor_directory}/.recipewrap",
      "workingdir": "{bfactor_directory}/"
    },
    "queue": "cluster.submission",
    "service": "RefineWrapper"
  },
  "4": {
    "output": {
      "ispyb_connector": 6,
      "murfey_feedback": 5,
      "node_creator": 2
    },
    "parameters": {
      "picker_id": "{picker_id}",
      "refined_class_uuid": "{refined_class_uuid}",
      "refined_grp_uuid": "{refined_grp_uuid}"
    },
    "queue": "postprocess",
    "service": "PostProcess"
  },
  "5": {
    "parameters": {
      "program_id": "{autoproc_program_id}",
      "session_id": "{session_id}"
    },
    "queue": "{feedback_queue}",
    "service": "Murfey"
  },
  "6": {
    "parameters": {
      "dcid": "{dcid}",
      "ispyb_command": "multipart_message",
      "program_id": "{autoproc_program_id}"
    },
    "queue": "ispyb_connector",
    "service": "EMISPyB"
  },
  "start": [[1, []]]
}
